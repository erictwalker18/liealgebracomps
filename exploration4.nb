Cell["\<\
The command below outputs the standard basis elements of 5x5 matrices (B[i,j] \
has a 1 in the ij entry and zeros elsewhere).  It should be clear how to \
change it for different nxn matrices.\
\>", "Section",
 CellChangeTimes->{{3.685806251007826*^9, 3.685806296274*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"B", "[", 
   RowBox[{"i_", ",", "j_"}], "]"}], ":=", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"Piecewise", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{
          RowBox[{"i", "\[Equal]", "m"}], "&&", 
          RowBox[{"j", "\[Equal]", "n"}]}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", "True"}], "}"}]}], "}"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"m", ",", "4"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"n", ",", "4"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.68580143627414*^9, 3.685801620453567*^9}, {
  3.685801674349395*^9, 3.685801698468697*^9}, {3.685810531444718*^9, 
  3.685810534132803*^9}}],

Cell[BoxData[
 RowBox[{
